{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalItems": false,
  "properties": {
    "id": {
      "type": "string",
      "description": "Faculty unique ID."
    },
    "short_name": {
      "type": "string",
      "description": "Faculty unique codename."
    },
    "name": {
      "type": "string",
      "description": "Faculty name."
    },
    "locales": {
      "type": "object",
      "description": "Faculty related locales."
    },
    "alt": {
      "type": "object",
      "description": "Alternative values for the faculty.",
      "properties": {
        "short_name": {
          "type": "array",
          "description": "Faculty alternative unique codenames.",
          "items": {
            "type": "string"
          }
        },
        "name": {
          "description": "Faculty alternative name.",
          "type": "string"
        }
      }
    }
  }

}
